<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Magento_CatalogRule::promo" title="Promotions" translate="title" sortOrder="10">
                      	<resource id="Magebees_Productlabel::label" title="Manage Labels" sortOrder="50" />
                    </resource>
                </resource>
				<resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magebees_Productlabel::configuration_settings" title="Product Labels Settings"  />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>